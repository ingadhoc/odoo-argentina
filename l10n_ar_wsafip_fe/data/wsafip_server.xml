    <!-- Update Concept Action -->
    <record id="action_server_wsfe_update_afip_concept_type" model="ir.actions.server">
            <field name="name">Update Concept</field>
            <field name="condition">obj.server_id.code == 'wsfe'</field>
            <field name="type">ir.actions.server</field>
            <field name="model_id" ref="l10n_ar_wsafip.model_wsafip_connection" />
            <field name="state">code</field>
            <field name="code">obj.server_id.wsfe_update_afip_concept_type(obj.id)</field>
    </record>

    <record model="ir.values" id="action_wsfe_update_afip_concept">
            <field name="name">action_module_wsfe_update_afip_concept_type</field>
            <field name="action_id" ref="action_server_wsfe_update_afip_concept_type" />
            <field name="value" eval="'ir.actions.server,' + str(ref('action_server_wsfe_update_afip_concept_type'))" />
            <field name="key">action</field>
            <field name="model_id" ref="l10n_ar_wsafip.model_wsafip_connection" />
            <field name="model">wsafip.connection</field>
            <field name="key2">client_action_multi</field>
    </record>

