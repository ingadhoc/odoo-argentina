<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

    <record model='wsafip.server' id='wsfe_prod'>
        <field name='name'>Factura Electrónica</field>
        <field name='code'>wsfe</field>
	    <field name='class'>production</field>
	    <field name='url'>https://servicios1.afip.gov.ar/wsfev1/service.asmx</field>
	</record>

